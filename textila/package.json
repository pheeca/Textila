{
  "name": "textila",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postbuild": "npm run createdir && npm run copytoserver",
    "createdir": "shx mkdir -p ../Textila.Web/Content ../Textila.Web/Scripts ../Textila.Web/fonts ../Textila.Web/img ../Textila.Web/Views/Home",
    "copytoserver":"shx cp -r dist\\Content\\* ../Textila.Web/Content && shx cp -r dist\\Scripts\\* ../Textila.Web/Scripts && shx cp -r dist\\fonts\\* ../Textila.Web/fonts && shx cp -r dist\\img\\* ../Textila.Web/img && shx cp -r dist\\*.ico ../Textila.Web/ && shx cp -r dist\\*.html ../Textila.Web/Views/Home"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.4.4",
    "material-design-icons-iconfont": "^5.0.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.2",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "shx": "^0.3.2",
    "vue-cli-plugin-vuetify": "^2.0.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
